<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yooz.myapplication">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NextActivity" />
        <activity android:name=".LinearLayoutActivity" />
        <activity android:name=".DynamicLayout" />
        <activity android:name=".Scroll" />
        <activity
            android:name=".ButtonListActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".MapActivity" />
        <activity android:name=".SampleListViewActivity" />
        <activity android:name=".SimpleAdapter2Activity" />
        <activity android:name=".FragmentPagerActivity" />
        <activity android:name=".MyPager" />
        <activity android:name=".ScrollContinuousActivity" />
        <activity android:name=".FakeHiddenMenuActivity"></activity>
    </application>

</manifest>